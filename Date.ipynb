   "source": [
    "import pandas as pd\n",
    "import numpy as np\n",
    "import re\n",
    "\n",
    "with open('result.txt','r') as f:\n",
    "    data = f.read()\n",
    "\n",
    "date=[]\n",
    "for i in re.findall(r'[1-9]{2}\\s\\w+\\s[0-9]{4}', data):\n",
    "    date.append(i)\n",
    "print(\"Date: \",date[0])"
   ]
